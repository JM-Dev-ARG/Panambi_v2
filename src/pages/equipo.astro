---
import Layout from "@/layouts/Layout.astro";
import data from "@/data/card_hero_equipo.json";
import imgContacto from "@/assets/img/hero/img_equipo_hero.avif";
import HeroConCard from "@/components/base-components/HeroConCard.astro";
import EquipoGrid from "@/components/pages/equipo/sections/EquipoGrid.astro";
import UltimoLLamado from "@/components/base-components/UltimoLLamado.astro";
const keywords = [
  "equipo Panambí",
  "nuestro equipo profesional",
  "equipo de consultoría estratégica",
  "diseñadores y desarrolladores web",
  "equipo de marketing digital",
  "especialistas en estrategia digital",
  "trabajo en equipo Panambí",
  "liderazgo Panambí",
  "equipo creativo de marca",
  "profesionales desarrollo web Panambí",
  "equipo experto en posicionamiento online",
  "equipo de branding y diseño digital",
  "equipo de consultoría para profesionales",
];
---

<Layout
  title="Panambi - Equipo"
  keywords={keywords}
  description="Estrategia, diseño y tecnología para profesionales que quieren destacar. Conocé al equipo de Panambí: los especialistas que transforman tu marca y servicio digital."
>
  <HeroConCard
    imgCard={imgContacto.src}
    titulo="quienes somos"
    subtitulo="Conoce Un poco más de nosotros"
    dataText={data}
    bgColor="bg-negro"
  />
  <EquipoGrid />
  <UltimoLLamado
    idContainer="unite_equipo"
    textoBoton="Envianos tu CV"
    titulo="somos panambi"
    subtitulo="unite al equipo"
    hrefBoton="mailto:administracion@panambi.net"
    bgColor="bg-transparent"
  />
</Layout>

<script>
  import AOS from "aos";
  import "aos/dist/aos.css";
  document.addEventListener("astro:page-load", () => {
    AOS.init({
      once: true,
      duration: 800,
    });
  });
</script>
